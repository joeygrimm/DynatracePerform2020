                        "body": {
                            "mode": "raw",
                            "raw": "{\r\n  \"displayName\": \"my.coffees\",\r\n  \"unit\": \"Count\",\r\n  \"dimensions\": [\r\n    \"office\"\r\n  ],\r\n  \"types\": [\r\n    \"coffee machine\"\r\n  ]\r\n}",
